{
  "manifest_version": 3,
  "background": {
    "service_worker": "sw.js"
  },
  "action": {
    "default_title": "Open Search"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": [
    "sidePanel"
  ],
  "devtools_page": "index.html",
  "host_permissions": ["http://*/*","https://*/*"]
}